{
  "name": "sds-social",
  "version": "1.0.0",
  "type": "module",
  "description": "MERN authentication system",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "build": "npm i --legacy-peer-deps && npm i --legacy-peer-deps --prefix frontend && npm run build --prefix frontend",
    "client": "npm run dev --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "SDS",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.5.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "http": "^0.0.1-security",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.1.0",
    "react-swipeable": "^7.0.1",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "concurrently": "^8.0.1",
    "nodemon": "^3.0.1",
    "vite-plugin-pwa": "^0.16.5"
  }
}
