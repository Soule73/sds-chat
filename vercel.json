{
  "installCommand": "npm install --legacy-peer-deps && npm install --legacy-peer-deps --prefix frontend",
  "buildCommand": "npm run build --prefix frontend",
  "builds": [
    {
      "src": "/backend/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/backend/server.js"
    }
  ]
}
